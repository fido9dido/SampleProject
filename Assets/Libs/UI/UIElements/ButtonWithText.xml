<UIElements name="UI"> // Element for category identification

	<UIElement name="ButtonWithText" render_lockness="1" mouseevents="1" cursor="1">

		<GFx file="ButtonWithText.gfx" layer="1">
			<Constraints>
				<Align mode="dynamic" vAlign="mid" hAlign="mid" scale="1" />
			</Constraints>
		</GFx>
		<functions>
			<function name="SetButtonText" funcname="SetButtonText" desc="Set Button Text">
				<in_param name="newText" desc="Button Text" type="string"/>
			</function>
			<function name="GetButtonText" funcname="GetButtonText" desc="Get Button Text">
				<out_param  type="string"/>
			</function>
			<function name="SetButtonWidthAndHeight" funcname="SetButtonWidthAndHeight" desc="Set Button Width and Height">
				<in_param name="newWidth" desc="Button width" type="int"/>
				<in_param name="newHeight" desc="Button height" type="int"/>
			</function>
			<function name="SetButtonWidth" funcname="SetButtonWidth" desc="Set Button Width">
				<in_param name="newWidth" desc="Button Text" type="int"/>
			</function>			
			<function name="SetButtonHeight" funcname="SetButtonHeight" desc="Set Button Height">
				<in_param name="newHeight" desc="Button Text" type="int"/>
			</function>
				<function name="SetButtonPosition" funcname="SetButtonPosition" desc="Set Button Position">
				<in_param name="xPos" desc="x Position" type="int"/>
				<in_param name="yPos" desc="y Position" type="int"/>
			</function>	
	      	<function name="SetButtonVisible" funcname="SetButtonVisible" desc="Set Button Visible">
				<in_param name="value" desc="button Visiblility" type="bool"/>
			</function>
		</functions>

		<events>
			<event name="OnButtonPressed" fscommand ="OnPressed">
 				
				<param name="Name" desc="" type="string"/>
			</event>
		</events>
	
	</UIElement>
</UIElements>